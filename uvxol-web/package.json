{
  "name": "uvxol-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --silent",
    "server": "set NODE_OPTIONS=\"--trace-warnings\" && ts-node-dev --respawn --project server/tsconfig.json server/serve.ts",
    "build": "vue-cli-service build",
    "watch": "vue-cli-service build --watch",
    "lint": "vue-cli-service lint",
    "auth": "az login",
    "deploy": "npm run build && az storage blob upload-batch -s dist -d '$web' --account-name uvxolweb"
  },
  "dependencies": {
    "@types/ws": "^6.0.4",
    "core-js": "^2.6.5",
    "express": "^4.17.1",
    "fp-ts": "^2.0.5",
    "lambda-designer-js": "^1.0.19",
    "request-promise-native": "^1.0.7",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.2.2",
    "vue-router": "^3.0.3",
    "vue-socket.io": "MetinSeylan/Vue-Socket.io",
    "vuetify": "^2.0.0",
    "vuex": "^3.1.1",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/request-promise-native": "^1.0.16",
    "@types/socket.io-client": "^1.4.32",
    "@vue/cli-plugin-typescript": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "monocle-ts": "^2.0.0",
    "sass": "^1.17.4",
    "sass-loader": "^7.1.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.4.3",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2",
    "vuex-module-decorators": "^0.10.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
